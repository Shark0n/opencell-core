<enunciate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" slug="meveoApi" xsi:noNamespaceSchemaLocation="http://enunciate.webcohesion.com/schemas/enunciate-2.2.0.xsd">
    <title>opencell REST and WS API</title>
    <description>opencell API</description>

    <api-classes>
        <include pattern="org.meveo.api.**" />
    </api-classes>

    <modules>
        <jackson disabled="false" honorJaxb="true" />
        <jackson1 disabled="true" />
        <spring-web disabled="true" />
        <c-xml-client disabled="true" />
        <csharp-xml-client disabled="true" />
        <java-xml-client disabled="true" />
        <java-json-client disabled="true" />
        <gwt-json-overlay disabled="true" />
        <obj-c-xml-client disabled="true" />
        <php-xml-client disabled="true" />
        <php-json-client disabled="true" />
        <ruby-json-client disabled="true" />
        <idl disabled="true" />
        <jaxrs groupBy="path" />
        <docs docsSubdir="api" />
        <swagger host="i.meveo.info" basePath="/meveo/api/rest" docsSubdir="swagger" />
    </modules>
</enunciate>